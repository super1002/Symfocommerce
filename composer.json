{
    "description": "symfony framework based ecommerce project",
    "name": "morozovalexander/symfocommerce",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Morozov Alexander",
            "email": "webdevelopacc@gmail.com",
            "homepage": "https://github.com/morozovalexander/symfocommerce",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/common": "^2.12",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/user-bundle": "2.x-dev",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/knp-paginator-bundle": "^5.1",
        "liip/imagine-bundle": "^2.3",
        "oneup/uploader-bundle": "^2.1",
        "presta/sitemap-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.6",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/translation-contracts": "^1.1.6",
        "symfony/yaml": "^4.0",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "liip/functional-test-bundle": "~2.0@alpha",
        "symfony/debug-bundle": "^4.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/var-dumper": "^4.0",
        "symfony/web-profiler-bundle": "^4.0"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
