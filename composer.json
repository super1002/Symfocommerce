{
    "description": "symfony framework based ecommerce project",
    "name": "morozovalexander/symfocommerce",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Morozov Alexander",
            "email": "webdevelopacc@gmail.com",
            "homepage": "https://github.com/morozovalexander/symfocommerce",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "doctrine/common": "^2.12",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/user-bundle": "^2.1",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/knp-paginator-bundle": "^2.5",
        "liip/imagine-bundle": "^2.3",
        "oneup/uploader-bundle": "^2.1",
        "presta/sitemap-bundle": "^1.5",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.6",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/yaml": "^4.0",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "liip/functional-test-bundle": "~2.0@alpha",
        "symfony/debug-bundle": "^4.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/var-dumper": "^4.0",
        "symfony/web-profiler-bundle": "^4.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        },
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
